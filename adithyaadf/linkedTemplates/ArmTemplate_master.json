{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adithyaadf"
		},
		"AzureSqlsampledbLS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlsampledbLS'"
		},
		"containeradf_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'containeradf'"
		},
		"demodbls_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'demodbls'"
		},
		"sampledb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'sampledb'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adithyaadf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlsampledbLS_connectionString": {
						"value": "[parameters('AzureSqlsampledbLS_connectionString')]"
					},
					"containeradf_connectionString": {
						"value": "[parameters('containeradf_connectionString')]"
					},
					"demodbls_connectionString": {
						"value": "[parameters('demodbls_connectionString')]"
					},
					"sampledb_connectionString": {
						"value": "[parameters('sampledb_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}